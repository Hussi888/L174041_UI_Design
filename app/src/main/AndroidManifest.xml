<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.uidesign">

    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Design.NoActionBar">
        <activity android:name="com.example.uidesign.DeleteContact"></activity>
        <activity android:name="com.example.uidesign.AddContact" />
        <activity android:name="com.example.uidesign.ContactActivity" />
        <activity android:name="com.example.uidesign.MainActivity" />
        <activity android:name="com.example.uidesign.ListActivity" />
        <activity
            android:name="com.example.uidesign.LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>